{
 "$schema": "node_modules/wrangler/config-schema.json",
  "name": "usher-mcp",
  "main": "server/index.ts",
  "compatibility_date": "2025-11-30",
 "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": "./web/dist",
    "binding": "ASSETS",
 },
 "observability": {
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "invocation_logs": true,
      "persist": true
    }
  },
  "build": {
    "command": "npm run web:build"
  }
}